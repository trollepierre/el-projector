{
  "name": "el-projector-back",
  "version": "0.0.1",
  "description": "GTD helper",
  "author": "Pierre Troll√© <trolle.pierre@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "yarn clean:dependencies && yarn clean:db",
    "clean:dependencies": "rm -rf node_modules",
    "clean:db": "yarn save:db && rm src/db/data.development.sqlite",
    "save:db": "node ./bin/save-db.js",
    "init:db": "node ./bin/init-db.js",
    "lint": "eslint --ext .js src",
    "lint:fix": "yarn lint --fix",
    "start": "node ./bin/www",
    "start:watch": "nodemon ./bin/www",
    "test": "yarn test:unit --coverage",
    "test:unit": "env TZ=\"UTC\" jest --config config/jest.config.json"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "date-fns": "2.2.1",
    "debug": "4.1.1",
    "dotenv-extended": "^2.4.0",
    "express": "4.17.1",
    "http-errors": "1.7.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "morgan": "1.9.1",
    "pg": "^7.12.1",
    "ramda": "0.26.1",
    "sequelize": "5.19.0",
    "sqlite3": "4.1.0"
  },
  "devDependencies": {
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "jest": "24.9.0",
    "lolex": "4.2.0",
    "nodemon": "^1.19.2",
    "supertest": "4.0.2"
  }
}
