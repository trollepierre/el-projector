{
  "name": "el-projector-back",
  "version": "0.0.1",
  "description": "Relations helper",
  "author": "Pierre Troll√© <trolle.pierre@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "yarn clean:dependencies && yarn clean:db",
    "clean:dependencies": "rm -rf node_modules",
    "clean:db": "yarn save:db && rm src/db/data.development.sqlite",
    "save:db": "node ./bin/save-db.js",
    "init:db": "node ./bin/init-db.js",
    "lint": "eslint --ext .js src",
    "lint:fix": "yarn lint --fix",
    "start": "node ./bin/www",
    "start:watch": "nodemon ./bin/www",
    "test": "yarn test:unit --coverage",
    "test:unit": "env TZ=\"UTC\" jest --config config/jest.config.json"
  },
  "dependencies": {
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "date-fns": "2.0.0-alpha.27",
    "debug": "4.1.1",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "http-errors": "1.7.1",
    "morgan": "1.9.1",
    "ramda": "0.26.1",
    "sequelize": "4.42.0",
    "sqlite3": "4.0.6"
  },
  "devDependencies": {
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "jest": "23.6.0",
    "lolex": "3.1.0",
    "nodemon": "1.18.9",
    "supertest": "3.4.1"
  }
}
