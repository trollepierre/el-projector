{
  "version": 2,
  "builds": [
    { "src": "back/*.js", "use": "@now/node" },
    { "src": "front/*.html", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/back/api/$1" },
    { "src": "/apo/(.*)", "dest": "/back/apo/$1" },
    { "src": "/(.*)", "dest": "/front/$1" }
  ]
}
